{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/dark-gothic/(.*)",
      "destination": "/dark-gothic/$1"
    },
    {
      "source": "/demon-slayer/(.*)",
      "destination": "/demon-slayer/$1"
    },
    {
      "source": "/one-piece/(.*)",
      "destination": "/one-piece/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
